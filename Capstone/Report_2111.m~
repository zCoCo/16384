function Report_2111()
    deg = pi/180;
    
    qA = [45*deg; 2; 3];
    dhpA = [...
        0,			-pi/2,		1,				0;		...
        0,			pi,			0,				0;		...
        0,          -pi/2,      2.5e-3,			-pi/2;	...
    ];
    actuatedJointsA = [4;3;3]; % Column Indices within Each DHP Row, i, to which the ith Entry in the Joint State Vector Gets Added
    robotA = Robot3D(dhpA, actuatedJointsA);
    DHP.transform(robotA.dhp.data, qA)

    qB = [0; 2; 3; 1; 20*deg; 45*deg; 90*deg]; % Prepend with 0 to account for static transform from {0} to {1}
    dhpB = [...
        0,			-pi/2,		1,				0;		...
        0,			pi/2,		1,				-pi/2;	...
        0,          -pi/2,      1,				-pi/2;	...
        0,			pi,			0,				0;		...
        0,			-pi/2,		0,				0;		...
        0,			pi/2,		0,				0;		...
        0,			pi,			0,				0 		...
    ];
    actuatedJointsB = [0;3;3;3;4;4;4]; % Column Indices within Each DHP Row, i, to which the ith Entry in the Joint State Vector Gets Added
    robotB = Robot3D(dhpB, actuatedJointsB);
    DHP.transform(robotB.dhp.data, qB)

    qC = [20*deg; 45*deg; 45*deg; -45*deg; 90*deg];
    dhpC = [...
        0,			pi/2,		116.23e-3,		0;		...
        327.76e-3,	pi,			0,				0;		...
        0,          -pi/2,      2.5e-3,			-pi/2;	...
        254.10e-3,	pi/2,		94.05e-3,		-pi/2;	...
        266.70e-3,	pi/2,		54.23e-3,		0		...
    ];
    actuatedJointsC = 4 * ones(size(dhp,1),1);
    robotC = Robot3D(dhpC, actuatedJointsC);
    DHP.transform(robotC.dhp.data, qC)
    
end % #Report_2111