function sandbox()
    dt = 0.01;
    traj = CJT('sine.csv', 10, 10, 10, dt);
    traj.dist(end)
    traj.params
    vi = zeros(size(traj.data.xs));
    vi(2:end) = diff(traj.data.xs));
    vi(1) = vi(2);
    ai = zeros(size(traj.data.vs));
    ai(2:end) = diff(traj.data.vs)/dt;
    ai(1) = ai(2);
    
    figure();
    hold on
        plot(traj.data.ts, traj.data.xs);
        plot(traj.data.ts, traj.data.vs);
        plot(traj.data.ts, vi);
        plot(traj.data.ts, traj.data.as);
        plot(traj.data.ts, ai);
        %plot(traj.data.ts, traj.data.js);
    hold off
    xlabel('Time [s]');
    legend('Position', 'Velocity', 'Num. Vel.', 'Acceleration', 'Jerk');
    
end % #sandbox